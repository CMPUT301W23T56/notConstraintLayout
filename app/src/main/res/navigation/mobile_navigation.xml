<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"

    app:startDestination="@+id/navigation_dashboard">

    <fragment
        android:id="@+id/navigation_dashboard"
        android:name="com.example.notconstraintlayout.ui.dashboard.DashboardFragment"
        android:label="Dashboard"
        tools:layout="@layout/fragment_dashboard" />

    <fragment
        android:id="@+id/navigation_explore"
        android:name="com.example.notconstraintlayout.ui.explore.ExploreFragment"
        android:label="Explore"

        tools:layout="@layout/fragment_explore" />

    <fragment
        android:id="@+id/navigation_leaderboard"
        android:name="com.example.notconstraintlayout.ui.leaderboard.LeaderBoardFragment"
        android:label="Leaderboard"
        tools:layout="@layout/fragment_leaderboard" />
</navigation>